  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "prefer_related_applications": false,
  
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=gov.punjab.nabha.health",
      "id": "gov.punjab.nabha.health"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/nabha-healthconnect/id1234567890",
      "id": "1234567890"
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "tel",
      "url": "/enhanced-nabha-health-pwa.html?action=call&number=%s"
    },
    {
      "protocol": "mailto",
      "url": "/enhanced-nabha-health-pwa.html?action=email&address=%s"
    }
  ],
  
  "file_handlers": [
    {
      "action": "/enhanced-nabha-health-pwa.html?action=open-prescription",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".jpg", ".jpeg", ".png", ".webp"]
      }
    }
  ],
  
  "share_target": {
    "action": "/enhanced-nabha-health-pwa.html?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "prescription",
          "accept": ["image/*", "application/pdf"]
        }
      ]
    }
  },
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  
  "handle_links": "preferred",
  
  "scope_extensions": [
    {"origin": "https://api.nabha-health.gov.in"},
    {"origin": "https://nabha-health.gov.in"}
  ],
  
  "categories_custom": [
    "rural-healthcare",
    "telemedicine",
    "punjab-government",
    "digital-health",
    "emergency-services"
  ],
  
  "permissions": [
    "camera",
    "microphone",
    "geolocation",
    "notifications",
    "background-sync"
  ],
  
  "offline_enabled": true,
  "orientation_lock": false,
  "fullscreen": false,
  "minimal_ui": true
}